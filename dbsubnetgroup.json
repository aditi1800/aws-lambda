{
  "AWSTemplateFormatVersion": "2010-09-09",
  "Parameters": {
    "SubnetId": {
      "Type": "String"
    }
  },
  "Resources": {
    "myDBSubnetGroup": {
      "Type": "AWS::RDS::DBSubnetGroup",
      "Properties": {
        "DBSubnetGroupDescription": "description",
        "SubnetIds": {
          "Fn::Split": [
            ",",
            {
              "Ref": "SubnetId"
            }
          ]
        },
        "Tags": [
          {
            "Key": "Name",
            "Value": "DemoSubnetGroup"
          }
        ]
      }
    }
  }
}
